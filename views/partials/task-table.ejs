<table class="table">
  <thead>
    <tr>
      <th scope="col">Nama</th>
      <th scope="col">Deskripsi</th>
      <th scope="col">Status</th>
      <th scope="col">Aksi</th>
    </tr>
  </thead>
  <tbody>
    <% tasks.forEach(task => { %>

    <tr>
      <td><%= task.name %></td>
      <td><%= task.description %></td>
      <td><%= task.status %></td>
      <td>
        <button class="btn btn-primary">Selesai</button>
      </td>
    </tr>

    <% }); %>
  </tbody>
</table>
